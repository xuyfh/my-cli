{
  "presets": [
    ["@babel/preset-env", {
      // 使用的api会自动转化，并且是按需加载
      "useBuiltIns": "usage",
      // babel-polyfill
      "corejs": 2
    }],
    "@babel/preset-react",
    ["@babel/preset-typescript", {
      "allExtensions": true
    }]
  ],
  "plugins": [
    // 解析装饰器
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    // 解析类的属性
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    // 用来整理代码，预防多次相同的转译
    "@babel/plugin-transform-runtime"
  ]
}